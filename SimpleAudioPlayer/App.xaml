<Application
  x:Class="SimpleAudioPlayer.App"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:l="clr-namespace:SimpleAudioPlayer"
  ShutdownMode="OnMainWindowClose"
  StartupUri="View/MainWindow.xaml">
  <Application.Resources>
    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    <l:TimeSpan2ShortTimeConverter x:Key="TimeSpan2ShortTimeConverter" />
    <l:TimeSpan2TotalSecondsConverter x:Key="TimeSpan2TotalSecondsConverter" />
    <l:InverseBooleanToVisibilityConverter x:Key="InverseBooleanToVisibilityConverter" />
  </Application.Resources>
</Application>
